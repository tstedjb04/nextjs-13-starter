{
    "semi": false,
    "tabWidth": 4,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "all",
    "importOrder": [
        "react",
        "next",
        "^next/(.*)$",
        "<THIRD_PARTY_MODULES>",
        "^@components/(.*)$",
        "^@helpers/(.*)$",
        "^@utils/(.*)$",
        "^@types/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true
}